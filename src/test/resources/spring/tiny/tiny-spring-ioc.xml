<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="outputService" class="club.csiqu.framework.spring.tiny.service.impl.OutputServiceImpl">

    </bean>
    <bean id="helloService" class="club.csiqu.framework.spring.tiny.service.impl.HelloServiceImpl">
        <property name="text" value="Hello World!"/>
        <property name="outputService" ref="outputService"/>
    </bean>
</beans>