<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <context:annotation-config/>
    <context:component-scan base-package="club.csiqu.framework.spring.demo.ioc"/>
    <bean id="springHelloWorld"
          class="club.csiqu.framework.spring.demo.ioc.SpringHelloWorldImpl"/>
    <bean id="summerHelloWorld"
          class="club.csiqu.framework.spring.demo.ioc.SummerHelloWorldImpl"/>
    <bean id="helloWorldService"
          class="club.csiqu.framework.spring.demo.ioc.HelloWorldService">
        <property name="helloWorld" ref="springHelloWorld"/>
    </bean>

</beans>